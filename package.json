{
  "name": "rickipedia",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "gql-make": "yarn run gql-fetch && yarn run gql-generate",
    "gql-fetch": "apollo client:download-schema --endpoint ${GQL_URI:='https://rickandmortyapi.com/graphql/'} ./graphql.schema.json",
    "gql-generate": "apollo client:codegen --addTypename --outputFlat --localSchemaFile ./graphql.schema.json --target typescript --includes 'src/**/*.tsx' ./src/__generated/apollogen-types.ts"
  },
  "dependencies": {
    "@apollo/react-hooks": "^3.1.3",
    "apollo": "^2.21.0",
    "apollo-boost": "^0.4.4",
    "graphql": "^14.5.8",
    "idx": "^2.5.6",
    "react": "16.9.0",
    "react-native": "0.61.2",
    "react-native-gesture-handler": "^1.5.0",
    "react-native-paper": "^3.1.1",
    "react-native-reanimated": "^2.10.0",
    "react-native-screens": "^1.0.0-alpha.23",
    "react-native-vector-icons": "^6.6.0",
    "react-navigation": "^4.0.10",
    "react-navigation-stack": "^1.10.3"
  },
  "devDependencies": {
    "@apollo/react-testing": "^3.1.3",
    "@babel/core": "^7.6.2",
    "@babel/runtime": "^7.6.2",
    "@react-native-community/eslint-config": "^0.0.5",
    "@types/jest": "^24.0.18",
    "@types/react": "^16.8.0",
    "@types/react-native": "^0.60.21",
    "@types/react-test-renderer": "16.9.0",
    "apollo-cache-inmemory": "^1.6.2",
    "apollo-client": "^2.6.4",
    "apollo-link": "^1.2.12",
    "apollo-utilities": "^1.3.2",
    "babel-jest": "^24.9.0",
    "babel-plugin-idx": "^2.4.0",
    "eslint": "^6.6.0",
    "jest": "^24.9.0",
    "metro-react-native-babel-preset": "^0.56.0",
    "react-native-testing-library": "^1.11.1",
    "react-test-renderer": "16.9.0",
    "typescript": "^3.6.3"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
